select id, 
  xpath('/c:contact/c:NameCollection//c:FormattedName/text()',contact,'{{c,http://schemas.microsoft.com/Contact}}') as company ,
  xpath('/c:contact/c:EmailAddressCollection/c:EmailAddress/c:Address/text()',contact,'{{c,http://schemas.microsoft.com/Contact}}') as  address  
   from webinfo.contacts limit 100 ;
select id, 
  xpath('/c:contact/c:NameCollection//c:FormattedName/text()',contact,'{{c,http://schemas.microsoft.com/Contact}}') as company ,
  xpath('/c:contact/c:EmailAddressCollection/c:EmailAddress/c:Address/text()',contact,'{{c,http://schemas.microsoft.com/Contact}}') as  address  
   from webinfo.contacts order by company limit 100 ;
with extractd as (select id, 
  xpath('/c:contact/c:NameCollection//c:FormattedName/text()',contact,'{{c,http://schemas.microsoft.com/Contact}}') as company ,
  xpath('/c:contact/c:EmailAddressCollection/c:EmailAddress/c:Address/text()',contact,'{{c,http://schemas.microsoft.com/Contact}}') as  address  
   from webinfo.contacts  ) select company , address from extractd order by company   ;
with extractd as (select id, 
  xpath('/c:contact/c:NameCollection//c:FormattedName/text()',contact,'{{c,http://schemas.microsoft.com/Contact}}') as company ,
  xpath('/c:contact/c:EmailAddressCollection/c:EmailAddress/c:Address/text()',contact,'{{c,http://schemas.microsoft.com/Contact}}') as  address  
   from webinfo.contacts  ) select company , address from extractd ;
with extractd as (select id, 
  xpath('/c:contact/c:NameCollection//c:FormattedName/text()',contact,'{{c,http://schemas.microsoft.com/Contact}}') as company ,
  xpath('/c:contact/c:EmailAddressCollection/c:EmailAddress/c:Address/text()',contact,'{{c,http://schemas.microsoft.com/Contact}}') as  address  
   from webinfo.contacts  ) select company , address from extractd order by extracted.company ;
with extractd as (select id, 
  xpath('/c:contact/c:NameCollection//c:FormattedName/text()',contact,'{{c,http://schemas.microsoft.com/Contact}}') as company ,
  xpath('/c:contact/c:EmailAddressCollection/c:EmailAddress/c:Address/text()',contact,'{{c,http://schemas.microsoft.com/Contact}}') as  address  
   from webinfo.contacts  ) select company , address from extractd order by extractd.company ;
with extractd as (select id, 
 unnest( xpath('/c:contact/c:NameCollection//c:FormattedName/text()',contact,'{{c,http://schemas.microsoft.com/Contact}}')) as company ,
 unnest( xpath('/c:contact/c:EmailAddressCollection/c:EmailAddress/c:Address/text()',contact,'{{c,http://schemas.microsoft.com/Contact}}')) as  address  
   from webinfo.contacts  ) select company , address from extractd order by extractd.company ;
with extractd as (select id, 
 unnest( xpath('/c:contact/c:NameCollection//c:FormattedName/text()',contact,'{{c,http://schemas.microsoft.com/Contact}}')) as company ,
 unnest( xpath('/c:contact/c:EmailAddressCollection/c:EmailAddress/c:Address/text()',contact,'{{c,http://schemas.microsoft.com/Contact}}')) as  address  
   from webinfo.contacts  ) select company , address from extractd  ;
with extracted as (select id, 
  unnest(xpath('/c:contact/c:NameCollection//c:FormattedName/text()',contact,'{{c,http://schemas.microsoft.com/Contact}}'))::TEXT as company ,
  unnest(xpath('/c:contact/c:EmailAddressCollection/c:EmailAddress/c:Address/text()',contact,'{{c,http://schemas.microsoft.com/Contact}}'))::TEXT as  address  
   from webinfo.contacts) select company , address from extracted  limit 100 offset;
with extracted as (select id, 
  unnest(xpath('/c:contact/c:NameCollection//c:FormattedName/text()',contact,'{{c,http://schemas.microsoft.com/Contact}}'))::TEXT as company ,
  unnest(xpath('/c:contact/c:EmailAddressCollection/c:EmailAddress/c:Address/text()',contact,'{{c,http://schemas.microsoft.com/Contact}}'))::TEXT as  address  
   from webinfo.contacts) select company , address from extracted  limit 100 offset 0;
with extracted as (select id, 
  unnest(xpath('/c:contact/c:NameCollection//c:FormattedName/text()',contact,'{{c,http://schemas.microsoft.com/Contact}}'))::TEXT as company ,
  unnest(xpath('/c:contact/c:EmailAddressCollection/c:EmailAddress/c:Address/text()',contact,'{{c,http://schemas.microsoft.com/Contact}}'))::TEXT as  address  
   from webinfo.contacts) select company , address from extracted  order by company limit 100 offset 0;
